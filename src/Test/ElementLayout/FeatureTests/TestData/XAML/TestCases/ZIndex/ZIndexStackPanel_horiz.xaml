<StackPanel Orientation="Horizontal"  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Background="orange" HorizontalAlignment="Center" VerticalAlignment="Center">

  <StackPanel.Resources>
    <Style TargetType="{x:Type Button}">
      <Setter Property="Height" Value="100" />
      <Setter Property="Width" Value="100" />
    </Style>
  </StackPanel.Resources>

  <Button Content="one" Margin="0,0,0,0" Name="one_btn" Panel.ZIndex="1"/>
  <Button Content="two" Margin="-95,0,0,0" Name="two_btn" Panel.ZIndex="2"/>
  <Button Content="three" Margin="-95,0,0,0" Name="three_btn" Panel.ZIndex="3"/>
  <Button Content="four" Margin="-95,0,0,0" Name="four_btn" Panel.ZIndex="4"/>
  <Button Content="five" Margin="-95,0,0,0" Name="five_btn" Panel.ZIndex="5"/>


</StackPanel>
