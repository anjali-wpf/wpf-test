<Page WindowTitle="HyperlinkTargetingBVT"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Name="MainHyperlinkPage">

    <StackPanel Background="LightGreen">

      <TextBlock FontFamily="TimesNewRoman" FontSize="18" FontWeight="Bold">Hyperlink Targeting</TextBlock>
      <TextBlock FontFamily="TimesNewRoman" FontSize="14">Navigate to XAML Content</TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickNoTargetNameXaml" NavigateUri="pack://siteoforigin:,,,/FramePage_Loose.xaml">DoClickNoTargetNameXaml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickBlankXaml" TargetName="_blank" NavigateUri="pack://siteoforigin:,,,/FramePage_Loose.xaml">DoClickBlankXaml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickParentXaml" TargetName="_parent" NavigateUri="pack://siteoforigin:,,,/FramePage_Loose.xaml">DoClickParentXaml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickSelfXaml" TargetName="_self" NavigateUri="pack://siteoforigin:,,,/FramePage_Loose.xaml">DoClickSelfXaml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickUnrecognizedXaml" TargetName="no_element_has_this_name" NavigateUri="pack://siteoforigin:,,,/FramePage_Loose.xaml">DoClickUnrecognizedXaml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickTopXaml" TargetName="_top" NavigateUri="pack://siteoforigin:,,,/FramePage_Loose.xaml">DoClickTopXaml</Hyperlink>
      </TextBlock>

      <TextBlock FontFamily="TimesNewRoman" FontSize="14">Navigate to HTML Content</TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickNoTargetNameHtml" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">DoClickNoTargetNameHtml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickBlankHtml" TargetName="_blank" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">DoClickBlankHtml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickParentHtml" TargetName="_parent" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">DoClickParentHtml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickSelfHtml" TargetName="_self" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">DoClickSelfHtml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickUnrecognizedHtml" TargetName="no_element_has_this_name" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">DoClickUnrecognizedHtml</Hyperlink>
      </TextBlock>

      <TextBlock Width="150">
        <Hyperlink Name="DoClickTopHtml" TargetName="_top" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">DoClickTopHtml</Hyperlink>
      </TextBlock>

    </StackPanel>

</Page>