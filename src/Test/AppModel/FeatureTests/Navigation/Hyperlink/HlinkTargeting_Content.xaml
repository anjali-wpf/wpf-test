<Page WindowTitle="HyperlinkTargetingBVT"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Name="mainHyperlinkPage">

  <ScrollViewer>
    <StackPanel Background="LightGreen">

      <TextBlock FontFamily="TimesNewRoman" FontSize="18" FontWeight="Bold">Site of Origin (pack://siteoforigin:,,,/  prefix)</TextBlock>
      <TextBlock>Navigate to XAML Content</TextBlock>

      <TextBlock>
        <Hyperlink Name="DoClickNoTargetName" NavigateUri="pack://siteoforigin:,,,/LooseXaml_HomePage.xaml">IInvokeProvider NoTargetName test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="DoClickBlank" TargetName="_blank" NavigateUri="pack://siteoforigin:,,,/LooseXaml_HomePage.xaml">IInvokeProvider _blank test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="DoClickParent" TargetName="_parent" NavigateUri="pack://siteoforigin:,,,/LooseXaml_HomePage.xaml">IInvokeProvider _parent test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="DoClickSelf" TargetName="_self" NavigateUri="pack://siteoforigin:,,,/LooseXaml_HomePage.xaml">IInvokeProvider _self test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="DoClickUnrecognized" TargetName="no_element_has_this_name" NavigateUri="pack://siteoforigin:,,,/LooseXaml_HomePage.xaml">IInvokeProvider no_element_has_this_name test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="DoClickTop" TargetName="_top" NavigateUri="pack://siteoforigin:,,,/LooseXaml_HomePage.xaml">IInvokeProvider _top test</Hyperlink>
      </TextBlock>

      <TextBlock>Navigate to HTML Content</TextBlock>

      <TextBlock>
        <Hyperlink Name="ReqNavNoTargetName" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">RequestNavigate NoTargetName test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="ReqNavBlank" TargetName="_blank" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">RequestNavigate _blank test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="ReqNavParent" TargetName="_parent" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">RequestNavigate _parent test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="ReqNavSelf" TargetName="_self" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">RequestNavigate _self test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="ReqNavUnrecognized" TargetName="no_element_has_this_name" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">RequestNavigate no_element_has_this_name test</Hyperlink>
      </TextBlock>

      <TextBlock>
        <Hyperlink Name="ReqNavTop" TargetName="_top" NavigateUri="pack://siteoforigin:,,,/WebPage1_Loose.html">RequestNavigate _top test</Hyperlink>
      </TextBlock>

    </StackPanel>

  </ScrollViewer>

</Page>