<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
    <ProductVersion>9.0.30314</ProductVersion>
    <SchemaVersion>2.0</SchemaVersion>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>SplashScreenLibrary</RootNamespace>
    <AssemblyName>SplashScreenLibrary</AssemblyName>
    <FileAlignment>512</FileAlignment>
    <PublishDir>$(PublishDir)\SplashScreen\SplashTestLibrary</PublishDir>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>
  
    <ItemGroup>
      <ProjectReference Include="$(TestContractsProject)" />
      <ProjectReference Include="$(TestRuntimeProject)" />
    </ItemGroup>

  <ItemGroup>
    <Compile Include="SplashScreenUIHandler.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="..\Common\splash.bmp" />
    <Resource Include="..\Common\splash2.png" />
    <Resource Include="..\Common\splash3alpha.png" />
  </ItemGroup>

</Project>
