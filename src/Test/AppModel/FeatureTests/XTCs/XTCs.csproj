<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <NoTargets>true</NoTargets>
  </PropertyGroup>
  <ItemGroup>
    <None Include="Compilation\*.xtc">
      <Link>\XTCs\Compilation\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="Orcas\*.xtc">
      <Link>\Orcas\XTCs\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="*.xtc">
      <Link>\XTCs\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
