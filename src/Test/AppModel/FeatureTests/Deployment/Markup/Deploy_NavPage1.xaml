<Page xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    WindowTitle="Simple App Page 1"
    Background="Orange"
    >	
  <DockPanel LastChildFill="False" >

    <TextBlock DockPanel.Dock="Top">
        <Hyperlink Name="goXbapMainPage" NavigateUri="Deploy_BasicXbapMarkup.xaml">To Simple Xbap main Page</Hyperlink>
    </TextBlock>

    <TextBlock DockPanel.Dock="Top">
        <Hyperlink Name="goWebOCMainPage" NavigateUri="Deploy_WebOCAppMarkup.xaml">To WebOC .App main Page</Hyperlink>
    </TextBlock>

    <TextBlock DockPanel.Dock="Top">
        <Hyperlink Name="goWebOCMainPage2" NavigateUri="Deploy_WebOCXbapMarkup.xaml">To WebOC .xbap main Page</Hyperlink>
    </TextBlock>

    <TextBlock DockPanel.Dock="Top">
        <Hyperlink Name="goPage2" NavigateUri="Deploy_NavPage2.xaml">To Page 2</Hyperlink>
    </TextBlock>

    <TextBlock DockPanel.Dock="Top">
        <Hyperlink Name="goPage3" NavigateUri="Deploy_NavPage3.xaml">To Page 3</Hyperlink>
    </TextBlock>

    <Button Name="Page2Button" Height="100" Width="300" DockPanel.Dock="Top" />
    <TextBox Name="Page2TextBox" Height="100" Width="300" DockPanel.Dock="Top" />
    <TextBlock>
        This is Page 1
    </TextBlock>
  </DockPanel>
</Page>
