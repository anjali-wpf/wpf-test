<Page
  WindowTitle="WebOC App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  x:Class="Microsoft.Test.WPF.AppModel.Deployment.WebOCFTApp"
  Loaded="CheckURLParams"
  Name="MainPage" 
>
 <DockPanel>
        <Border Margin="10" DockPanel.Dock="Top" BorderThickness="2" BorderBrush="Black" Background="LightGray">
            <DockPanel Name="panel" VerticalAlignment="Top" Height="40" Margin="10">
                <Button Click="navWebOC" Name="navigateButton" > Navigate WebOC </Button>
                <TextBox Height="35" Name="txtBox" />
            </DockPanel>
        </Border>

        <Border Margin="10" DockPanel.Dock="Top" BorderThickness="2" BorderBrush="Black" Background="LightGray">
             <Frame Name="WebOCFrame" MaxHeight="300" />
        </Border>

        <Button DockPanel.Dock="Top" Height="30" Width="150" Name="FocusTestButton">
		        1453368 Regression Prevention Button
        </Button>

        <TextBlock>
             <Hyperlink DockPanel.Dock="Top" NavigateUri="Deploy_NavPage1.xaml" Name="page1Link">Go To Page 1</Hyperlink>
        </TextBlock>

 </DockPanel>
</Page>
