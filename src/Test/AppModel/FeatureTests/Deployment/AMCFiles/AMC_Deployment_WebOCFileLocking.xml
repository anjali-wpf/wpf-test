<AppMonitorConfig>
 <Using Namespace="Microsoft.Test.Deployment" Assembly="DeploymentCommonLibrary"/>
 <Using Namespace="Microsoft.Windows.Test.Client.AppSec.Deployment.CustomUIHandlers" Assembly="DeploymentCommonLibrary"/>
     <Steps>
	<TestLogStep Name="Web OC File Locking Test Log">
            <ActivationStep Method="launch" Scheme="Local" FileName="ExpressAppWebOC.xbap">
                <ActivationStep.UIHandlers>
                    <WebOCFileLockingHandler WindowTitle="regexp:(Navigation Completed)" ProcessName="IEXPLORE" />
                </ActivationStep.UIHandlers>
            </ActivationStep>
	</TestLogStep>
     </Steps>
</AppMonitorConfig>
