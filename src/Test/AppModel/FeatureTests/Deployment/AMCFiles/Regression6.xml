<AppMonitorConfig>
 <Using Namespace="Microsoft.Test.Deployment" Assembly="DeploymentCommonLibrary"/>
 <Using Namespace="Microsoft.Test.Deployment.CustomUIHandlers" Assembly="DeploymentCommonLibrary"/>
     <Steps>
	<TestLogStep Name="Regression test 6:  [Windows 7] : After opening loose xaml in IE, pressing Tab key throws exception">

         <ActivationStep Method="launch" Scheme="LOCAL" FileName="SimpleBrowserHostedApplication.xbap">
             <ActivationStep.UIHandlers>
                 <TabThroughHandler ProcessName="iexplore" WindowTitle="RegExp:(Simple Browser Hosted SEE Application)" />
             </ActivationStep.UIHandlers>
         </ActivationStep>

         <!-- This markup loads OK in v3, but using v4 ensures that resource strings match the tests being used -->
         <ActivationStep Method="launch" Scheme="LOCAL" FileName="deploy_markup1_v4.xaml">
             <ActivationStep.UIHandlers>
                 <TabThroughHandler ProcessName="iexplore" WindowTitle="RegExp:(Loose XAML 1)" />
             </ActivationStep.UIHandlers>
         </ActivationStep>

	</TestLogStep>
     </Steps>
</AppMonitorConfig>
