<DockPanel
 x:Uid="DockPanel_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Name="homeRoot"
    Width="760" Height="630">
    <DockPanel.Resources>
        <Storyboard x:Uid="Storyboard_1" x:Key="IntroStoryboard" Duration="0:0:33" FillBehavior="HoldEnd">
            <!-- rectangles -->
            <ParallelTimeline x:Uid="ParallelTimeline_1" FillBehavior="HoldEnd">
                <DoubleAnimation x:Uid="DoubleAnimation_54" Storyboard.TargetName="borderRect1" Storyboard.TargetProperty="(Rectangle.Opacity)" BeginTime="0:0:4" To="1" Duration="0:0:1" />
                <DoubleAnimation x:Uid="DoubleAnimation_55" Storyboard.TargetName="borderRect2" Storyboard.TargetProperty="(Rectangle.Opacity)" BeginTime="0:0:4.33" To="1" Duration="0:0:1" />
                <DoubleAnimation x:Uid="DoubleAnimation_56" Storyboard.TargetName="borderRect3" Storyboard.TargetProperty="(Rectangle.Opacity)" BeginTime="0:0:4.66" To="1" Duration="0:0:1" />
            </ParallelTimeline>

            <!-- fade out rectangles -->
            <ParallelTimeline x:Uid="ParallelTimeline_2" BeginTime="0:0:30" FillBehavior="HoldEnd">
                <DoubleAnimation x:Uid="DoubleAnimation_57" Storyboard.TargetName="borderRect1" Storyboard.TargetProperty="(Rectangle.Opacity)" To="0" Duration="0:0:1" />
                <DoubleAnimation x:Uid="DoubleAnimation_58" Storyboard.TargetName="borderRect2" Storyboard.TargetProperty="(Rectangle.Opacity)" To="0" Duration="0:0:1" />
                <DoubleAnimation x:Uid="DoubleAnimation_59" Storyboard.TargetName="borderRect3" Storyboard.TargetProperty="(Rectangle.Opacity)" To="0" Duration="0:0:1" />
            </ParallelTimeline>

            <!-- old car image (1) -->
            <ParallelTimeline x:Uid="ParallelTimeline_3" BeginTime="0:0:6" Duration="0:0:5" FillBehavior="HoldEnd">
                <!-- fade in -->
                <DoubleAnimation x:Uid="DoubleAnimation_60" Storyboard.TargetName="image1" Storyboard.TargetProperty="(Image.Opacity)" To="1" Duration="0:0:2" />
                <!-- slide-up -->
                <DoubleAnimation x:Uid="DoubleAnimation_61" Storyboard.TargetName="image1" Storyboard.TargetProperty="(Canvas.Top)" From="150" To="50" Duration="0:0:5" />
                <!-- fade out -->
                <DoubleAnimation x:Uid="DoubleAnimation_62" Storyboard.TargetName="image1" Storyboard.TargetProperty="(Image.Opacity)" BeginTime="0:0:3.5" From="1" To="0" Duration="0:0:1.5" />
            </ParallelTimeline>
            <!-- end old car image -->

            <!-- concept car image (2) -->
            <ParallelTimeline x:Uid="ParallelTimeline_4" BeginTime="0:0:10" Duration="0:0:5">
                <!-- fade in -->
                <DoubleAnimation x:Uid="DoubleAnimation_63" Storyboard.TargetName="image2" Storyboard.TargetProperty="(Image.Opacity)" To="1" Duration="0:0:2" />
                <!-- slide-up -->
                <DoubleAnimation x:Uid="DoubleAnimation_64" Storyboard.TargetName="image2" Storyboard.TargetProperty="(Canvas.Top)" From="50" To="150" Duration="0:0:5" />
                <!-- fade out -->
                <DoubleAnimation x:Uid="DoubleAnimation_65" Storyboard.TargetName="image2" Storyboard.TargetProperty="(Image.Opacity)" BeginTime="0:0:3.5" From="1" To="0" Duration="0:0:1.5" />
            </ParallelTimeline>
            <!-- end streamline image -->

            <!-- grand prix car image (3) -->
            <ParallelTimeline x:Uid="ParallelTimeline_5" BeginTime="0:0:18" Duration="0:0:5">
                <!-- fade in -->
                <DoubleAnimation x:Uid="DoubleAnimation_66" Storyboard.TargetName="image3" Storyboard.TargetProperty="(Image.Opacity)" To="1" Duration="0:0:2" />
                <!-- slide-up -->
                <DoubleAnimation x:Uid="DoubleAnimation_67" Storyboard.TargetName="image3" Storyboard.TargetProperty="(Canvas.Top)" From="150" To="50" Duration="0:0:5" />
                <!-- fade out -->
                <DoubleAnimation x:Uid="DoubleAnimation_68" Storyboard.TargetName="image3" Storyboard.TargetProperty="(Image.Opacity)" BeginTime="0:0:3.5" From="1" To="0" Duration="0:0:1.5" />
            </ParallelTimeline>
            <!-- grand prix car image -->

            <!-- rally car image (4) -->
            <ParallelTimeline x:Uid="ParallelTimeline_6" BeginTime="0:0:22" Duration="0:0:5">
                <!-- fade in -->
                <DoubleAnimation x:Uid="DoubleAnimation_69" Storyboard.TargetName="image4" Storyboard.TargetProperty="(Image.Opacity)" To="1" Duration="0:0:2" />
                <!-- slide-up -->
                <DoubleAnimation x:Uid="DoubleAnimation_70" Storyboard.TargetName="image4" Storyboard.TargetProperty="(Canvas.Top)" From="50" To="150" Duration="0:0:5" />
                <!-- fade out -->
                <DoubleAnimation x:Uid="DoubleAnimation_71" Storyboard.TargetName="image4" Storyboard.TargetProperty="(Image.Opacity)" BeginTime="0:0:3.5" From="1" To="0" Duration="0:0:1.5" />
            </ParallelTimeline>
            <!-- rally car image -->

            <!-- car frame image (5) -->
            <ParallelTimeline x:Uid="ParallelTimeline_7" BeginTime="0:0:26" Duration="0:0:5">
                <!-- fade in -->
                <DoubleAnimation x:Uid="DoubleAnimation_72" Storyboard.TargetName="image5" Storyboard.TargetProperty="(Image.Opacity)" To="1" Duration="0:0:2" />
                <!-- slide-up -->
                <DoubleAnimation x:Uid="DoubleAnimation_73" Storyboard.TargetName="image5" Storyboard.TargetProperty="(Canvas.Top)" From="150" To="50" Duration="0:0:5" />
                <!-- fade out -->
                <DoubleAnimation x:Uid="DoubleAnimation_74" Storyboard.TargetName="image5" Storyboard.TargetProperty="(Image.Opacity)" BeginTime="0:0:3.5" From="1" To="0" Duration="0:0:1.5" />
            </ParallelTimeline>
            <!-- car frame image -->

            <!-- A8 L logo -->
            <ParallelTimeline x:Uid="ParallelTimeline_8" BeginTime="0:0:30" Duration="0:0:5">
                <!-- fade in -->
                <DoubleAnimation x:Uid="DoubleAnimation_75" Storyboard.TargetName="image6" Storyboard.TargetProperty="(Image.Opacity)" To="1" Duration="0:0:2" />
                <!-- fade out -->
                <DoubleAnimation x:Uid="DoubleAnimation_76" Storyboard.TargetName="image6" Storyboard.TargetProperty="(Image.Opacity)" BeginTime="0:0:3.5" From="1" To="0" Duration="0:0:1.5" />
            </ParallelTimeline>
            <!-- A8 L logo -->

            <!-- cheesy text timelines -->                  
            <ParallelTimeline x:Uid="ParallelTimeline_9" FillBehavior="HoldEnd">

                <!-- all this time you were waiting for the perfect car, Zzzz -->
                <ParallelTimeline x:Uid="ParallelTimeline_10" BeginTime="0:0:1" FillBehavior="HoldEnd">
                    <!-- fade-in-->
                    <DoubleAnimation x:Uid="DoubleAnimation_77" Storyboard.TargetName="text1" Storyboard.TargetProperty="(TextBlock.Opacity)" To="1" Duration="0:0:1" />
                    <!-- fade-in-->
                    <DoubleAnimation x:Uid="DoubleAnimation_78" Storyboard.TargetName="text2" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:2" To="1" Duration="0:0:1" />
                    <!-- half fade-out -->
                    <DoubleAnimation x:Uid="DoubleAnimation_79" Storyboard.TargetName="text1" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:2" To="0.5" Duration="0:0:1" />
                    <!-- fade-out -->
                    <DoubleAnimation x:Uid="DoubleAnimation_80" Storyboard.TargetName="text1" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:10" To="0" Duration="0:0:0.5" />
                    <!-- fade-out -->
                    <DoubleAnimation x:Uid="DoubleAnimation_81" Storyboard.TargetName="text2" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:10" To="0" Duration="0:0:0.5" />
                </ParallelTimeline>
                
                <!-- all this time we were building it -->
                <ParallelTimeline x:Uid="ParallelTimeline_11" BeginTime="0:0:15" FillBehavior="HoldEnd">
                    <!-- fade-in-->
                    <DoubleAnimation x:Uid="DoubleAnimation_82" Storyboard.TargetName="text1" Storyboard.TargetProperty="(TextBlock.Opacity)" To="1" Duration="0:0:1" />
                    <!-- fade-in-->
                    <DoubleAnimation x:Uid="DoubleAnimation_83" Storyboard.TargetName="text3" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:2" To="1" Duration="0:0:1" />
                    <!-- half fade-out -->
                    <DoubleAnimation x:Uid="DoubleAnimation_84" Storyboard.TargetName="text1" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:2" To="0.5" Duration="0:0:1" />
                    <!-- fade-out -->
                    <DoubleAnimation x:Uid="DoubleAnimation_85" Storyboard.TargetName="text1" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:10" To="0" Duration="0:0:0.5" />
                    <!-- fade-out -->
                    <DoubleAnimation x:Uid="DoubleAnimation_86" Storyboard.TargetName="text3" Storyboard.TargetProperty="(TextBlock.Opacity)" BeginTime="0:0:10" To="0" Duration="0:0:0.5" />
                </ParallelTimeline>
                
            </ParallelTimeline>         
            <!-- end: cheesy text timelines -->
        </Storyboard>
    </DockPanel.Resources>

    <Grid x:Uid="Grid_1">

