    <Canvas
 x:Uid="Canvas_2" Name="homeCanvas"
        Opacity="0"
        Visibility="Hidden">
        
        <Image
 x:Uid="Image_7" Source="Home1.jpg"
            Canvas.Top="0"
            Canvas.Left="0" />

        <!-- ABANDON ALL HOPE YE WHO ENTER HERE -->
        <!-- (these geometry values defined by trial and error -->
        <Path x:Uid="Path_1" Name="myPath" Fill="#FFADD8E6">   <!--Light blue fill on the path to simulate a wave #FF2C8CD3-->         
            
            <Path.Opacity x:Uid="Opacity_1">
                 <!--key frames are awesome!-->
                <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_1" BeginTime="0:0:0" Duration="0:0:7" AutoReverse="True" RepeatBehavior="Forever">
                    <LinearDoubleKeyFrame x:Uid="LinearDoubleKeyFrame_1" KeyTime="0" Value="0" />                   
                    <LinearDoubleKeyFrame x:Uid="LinearDoubleKeyFrame_2" KeyTime="0.5" Value="0.05" />                  
                    <LinearDoubleKeyFrame x:Uid="LinearDoubleKeyFrame_3" KeyTime="1" Value="0.1" />
                    <LinearDoubleKeyFrame x:Uid="LinearDoubleKeyFrame_4" KeyTime="6" Value="0.1" />
                    <LinearDoubleKeyFrame x:Uid="LinearDoubleKeyFrame_5" KeyTime="6.5" Value="0.05" />
                    <LinearDoubleKeyFrame x:Uid="LinearDoubleKeyFrame_6" KeyTime="7" Value="0" />
                </DoubleAnimationUsingKeyFrames>
            </Path.Opacity>

            <Path.Data x:Uid="Data_1"> 
                <GeometryGroup x:Uid="GeometryGroup_1">                        
                        <CombinedGeometry x:Uid="CombinedGeometry_1" GeometryCombineMode="Exclude">
                            <!-- rings -->  
                            <CombinedGeometry.Geometry1 x:Uid="Geometry1_1">
                                <GeometryGroup x:Uid="GeometryGroup_2">
                                <!-- all-ring-line-up-with-pool rotation -->
                                <GeometryGroup.Transform x:Uid="Transform_1">
                                    <TransformGroup x:Uid="TransformGroup_1">
                                        <TransformGroup.Children>
                                            <RotateTransform x:Uid="RotateTransform_1" CenterX="300" CenterY="300" Angle="15" />                                    
                                            <TranslateTransform x:Uid="TranslateTransform_1" X="0" Y="7" />                                        
                                        </TransformGroup.Children>
                                    </TransformGroup>
                                </GeometryGroup.Transform>
                                    
                                <!-- first ring -->
                                <CombinedGeometry x:Uid="CombinedGeometry_2" GeometryCombineMode="Exclude">
                                        <!-- outside -->
                                        <CombinedGeometry.Geometry1 x:Uid="Geometry1_2">
                                            <EllipseGeometry x:Uid="EllipseGeometry_1" Center="300, 300">
                                                <EllipseGeometry.RadiusX x:Uid="RadiusX_1">
                                                    <DoubleAnimation x:Uid="DoubleAnimation_34" From="15" To="100" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                                </EllipseGeometry.RadiusX>
                                                
                                                <EllipseGeometry.RadiusY x:Uid="RadiusY_1">
                                                    <DoubleAnimation x:Uid="DoubleAnimation_35" From="10" To="25" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                                </EllipseGeometry.RadiusY>
                                            </EllipseGeometry>
                                        </CombinedGeometry.Geometry1>
                                    
                                        <!-- inside -->
                                    <CombinedGeometry.Geometry2 x:Uid="Geometry2_1">
                                        <EllipseGeometry x:Uid="EllipseGeometry_2" Center="300, 295">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_2">
                                                <DoubleAnimation x:Uid="DoubleAnimation_36" From="10" To="98" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>

                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_2">
                                                <DoubleAnimation x:Uid="DoubleAnimation_37" From="5" To="20" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry2>
                                </CombinedGeometry>
                                
                                <!-- second ring -->
                                <CombinedGeometry x:Uid="CombinedGeometry_3" GeometryCombineMode="Exclude">
                                    <!-- outside -->
                                    <CombinedGeometry.Geometry1 x:Uid="Geometry1_3">
                                        <EllipseGeometry x:Uid="EllipseGeometry_3" Center="295, 310">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_3">
                                                <DoubleAnimation x:Uid="DoubleAnimation_38" From="45" To="130" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>
                                            
                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_3">
                                                <DoubleAnimation x:Uid="DoubleAnimation_39" From="40" To="55" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry1>
                                    
                                    <!-- inside -->
                                    <CombinedGeometry.Geometry2 x:Uid="Geometry2_2">
                                        <EllipseGeometry x:Uid="EllipseGeometry_4" Center="295, 305">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_4">
                                                <DoubleAnimation x:Uid="DoubleAnimation_40" From="40" To="128" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>

                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_4">
                                                <DoubleAnimation x:Uid="DoubleAnimation_41" From="35" To="50" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry2>
                                </CombinedGeometry>

                                <!-- third ring -->
                                <CombinedGeometry x:Uid="CombinedGeometry_4" GeometryCombineMode="Exclude">
                                        <!-- outside -->
                                    <CombinedGeometry.Geometry1 x:Uid="Geometry1_4">
                                        <EllipseGeometry x:Uid="EllipseGeometry_5" Center="290, 330">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_5">
                                                <DoubleAnimation x:Uid="DoubleAnimation_42" From="75" To="170" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>
                                            
                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_5">
                                                <DoubleAnimation x:Uid="DoubleAnimation_43" From="70" To="95" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry1>
                                    
                                    <!-- inside -->
                                    <CombinedGeometry.Geometry2 x:Uid="Geometry2_3">
                                        <EllipseGeometry x:Uid="EllipseGeometry_6" Center="290, 325">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_6">
                                                <DoubleAnimation x:Uid="DoubleAnimation_44" From="70" To="168" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>

                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_6">
                                                <DoubleAnimation x:Uid="DoubleAnimation_45" From="65" To="90" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry2>
                                </CombinedGeometry>

                                <!-- fourth ring -->
                                <CombinedGeometry x:Uid="CombinedGeometry_5" GeometryCombineMode="Exclude">
                                    <!-- outside -->
                                    <CombinedGeometry.Geometry1 x:Uid="Geometry1_5">
                                        <EllipseGeometry x:Uid="EllipseGeometry_7" Center="285, 350">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_7">
                                                <DoubleAnimation x:Uid="DoubleAnimation_46" From="105" To="220" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>
                                            
                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_7">
                                                <DoubleAnimation x:Uid="DoubleAnimation_47" From="100" To="145" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry1>
                                    
                                    <!-- inside -->
                                    <CombinedGeometry.Geometry2 x:Uid="Geometry2_4">
                                        <EllipseGeometry x:Uid="EllipseGeometry_8" Center="285, 345">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_8">
                                                <DoubleAnimation x:Uid="DoubleAnimation_48" From="100" To="218" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>

                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_8">
                                                <DoubleAnimation x:Uid="DoubleAnimation_49" From="95" To="140" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry2>
                                </CombinedGeometry>

                                <!-- fifth ring -->
                                <CombinedGeometry x:Uid="CombinedGeometry_6" GeometryCombineMode="Exclude">
                                    <!-- outside -->
                                    <CombinedGeometry.Geometry1 x:Uid="Geometry1_6">
                                        <EllipseGeometry x:Uid="EllipseGeometry_9" Center="280, 370">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_9">
                                                <DoubleAnimation x:Uid="DoubleAnimation_50" From="135" To="270" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>
                                            
                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_9">
                                                <DoubleAnimation x:Uid="DoubleAnimation_51" From="130" To="195" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry1>
                                    
                                    <!-- inside -->
                                    <CombinedGeometry.Geometry2 x:Uid="Geometry2_5">
                                        <EllipseGeometry x:Uid="EllipseGeometry_10" Center="280, 365">
                                            <EllipseGeometry.RadiusX x:Uid="RadiusX_10">
                                                <DoubleAnimation x:Uid="DoubleAnimation_52" From="130" To="268" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusX>

                                            <EllipseGeometry.RadiusY x:Uid="RadiusY_10">
                                                <DoubleAnimation x:Uid="DoubleAnimation_53" From="125" To="190" Duration="0:0:14" BeginTime="0:0:0" RepeatBehavior="Forever" />
                                            </EllipseGeometry.RadiusY>
                                        </EllipseGeometry>
                                    </CombinedGeometry.Geometry2>
                                </CombinedGeometry>

                            </GeometryGroup>
                            </CombinedGeometry.Geometry1>
                            
                            <!-- rectangles for clipping -->
                                <!-- car side clipping rectangle -->
                                <!-- left end of pool clipping rectangle -->
                            <CombinedGeometry.Geometry2 x:Uid="Geometry2_6">
                                <GeometryGroup x:Uid="GeometryGroup_3">
                                    <RectangleGeometry x:Uid="RectangleGeometry_1">
                                        <RectangleGeometry.Rect x:Uid="Rect_1">
                                            <Rect x:Uid="Rect_2" X="50" Y="110" Width="600" Height="200" />
                                        </RectangleGeometry.Rect>  
                                        <RectangleGeometry.Transform x:Uid="Transform_2">
                                            <RotateTransform x:Uid="RotateTransform_2" CenterX="300" CenterY="300" Angle="17" />
                                        </RectangleGeometry.Transform>                                        
                                    </RectangleGeometry>
                                    
                                    <RectangleGeometry x:Uid="RectangleGeometry_2">
                                        <RectangleGeometry.Rect x:Uid="Rect_3">
                                            <Rect x:Uid="Rect_4" X="0" Y="100" Width="150" Height="110" />
                                        </RectangleGeometry.Rect>   
                                        
                                        <RectangleGeometry.Transform x:Uid="Transform_3">
                                            <RotateTransform x:Uid="RotateTransform_3" CenterX="300" CenterY="300" Angle="-27" />
                                        </RectangleGeometry.Transform>
                                    </RectangleGeometry>
                                </GeometryGroup>          
                            </CombinedGeometry.Geometry2>
                        </CombinedGeometry>    
                </GeometryGroup>
            </Path.Data>
        </Path>
    </Canvas>
    </Grid>
</DockPanel>