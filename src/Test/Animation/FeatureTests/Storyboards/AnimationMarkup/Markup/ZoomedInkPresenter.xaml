<Page
    Name        = "Root"
    Tag         = "Double 3 ScaleY"
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml"
    Background  = "LightGreen">
    
    <InkPresenter Name="zoomedInkPresenter" Strokes="">
        <InkPresenter.RenderTransform>
            <ScaleTransform x:Name="AnimatedAnimatable" CenterX="20" CenterY="20" />
        </InkPresenter.RenderTransform>
    </InkPresenter>

    <Page.Triggers>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <EventTrigger.Actions>
                <BeginStoryboard>
                    <BeginStoryboard.Storyboard>
                        <Storyboard>
                            <DoubleAnimation
                                Storyboard.TargetProperty   = "(InkPresenter.RenderTransform).(ScaleTransform.ScaleX)"
                                Storyboard.TargetName       = "zoomedInkPresenter"
                                From                        = "0.3"
                                To                          = "3"
                                BeginTime                   = "00:00:02"
                                Duration                    = "00:00:04"
                                  />
                            <DoubleAnimation
                                Storyboard.TargetProperty   = "ScaleY"
                                Storyboard.TargetName       = "AnimatedAnimatable"
                                From                        = "0.3"
                                To                          = "3"
                                BeginTime                   = "00:00:02"
                                Duration                    = "00:00:04"
                                  />
                        </Storyboard>
                    </BeginStoryboard.Storyboard>
                </BeginStoryboard>
            </EventTrigger.Actions>
        </EventTrigger>
    </Page.Triggers>

</Page>
