<CustomDockPanel xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <CustomDockPanel.Resources>
    <av:SolidColorBrush x:Key="BlueBrush">#FF0000FF</av:SolidColorBrush>
    <av:Style TargetType="av:Button" x:Key="{x:Type av:Button}">
      <av:Style.Resources>
        <av:ResourceDictionary />
      </av:Style.Resources>
      <av:Setter Property="av:TextElement.Foreground">
        <av:Setter.Value>
          <av:SolidColorBrush>#FFFF0000</av:SolidColorBrush>
        </av:Setter.Value>
      </av:Setter>
    </av:Style>
  </CustomDockPanel.Resources>
  <av:Button BorderBrush="#FF008000" Name="Button1" Background="{av:DynamicResource BlueBrush}" />
</CustomDockPanel>