    <Table Background="#FF00FF00">
      <TableRowGroup>
        <TableRow>
          <TableCell FontStretch="ExtraExpanded" FontFamily="Palatino Linotype">
            <Paragraph>FontStretch="ExtraExpanded"</Paragraph>
          </TableCell>
        </TableRow>
      </TableRowGroup>
    </Table>
    <Table>
      <TableRowGroup>
        <TableRow>
          <TableCell FontStretch="UltraExpanded" FontFamily="Palatino Linotype">
            <Paragraph>FontStretch="UltraExpanded"</Paragraph>
          </TableCell>
        </TableRow>
      </TableRowGroup>
    </Table>
  </FlowDocument>
</FlowDocumentScrollViewer>