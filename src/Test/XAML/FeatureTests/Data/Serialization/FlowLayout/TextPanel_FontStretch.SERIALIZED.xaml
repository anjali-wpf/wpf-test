<UniformGrid Rows="8" Width="640" HorizontalAlignment="Left" Background="#FFFFFFFF" VerticalAlignment="Top" Columns="2" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <UniformGrid.Resources>
    <Style TargetType="Paragraph" x:Key="{x:Type Paragraph}">
      <Style.Resources>
        <ResourceDictionary />
      </Style.Resources>
      <Setter Property="TextElement.FontFamily">
        <Setter.Value>
          <FontFamily>Palatino LinoType</FontFamily>
        </Setter.Value>
      </Setter>
    </Style>
  </UniformGrid.Resources>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype" Background="#FF00FF00">
      <Paragraph FontStretch="Normal">FontStretch="Normal"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype">
      <Paragraph FontStretch="UltraCondensed">FontStretch="UltraCondensed"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype" Background="#FF00FF00">
      <Paragraph FontStretch="ExtraCondensed">FontStretch="ExtraCondensed"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype">
      <Paragraph FontStretch="Condensed">FontStretch="Condensed"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype" Background="#FF00FF00">
      <Paragraph FontStretch="SemiCondensed">FontStretch="SemiCondensed"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype">
      <Paragraph FontStretch="Expanded">FontStretch="Expanded"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype" Background="#FF00FF00">
      <Paragraph FontStretch="ExtraExpanded">FontStretch="ExtraExpanded"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument FontFamily="Palatino Linotype">
      <Paragraph FontStretch="UltraExpanded">FontStretch="UltraExpanded"</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
</UniformGrid>