<Section FontFamily="Arial" FontSize="16" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Table BorderBrush="#FF008000" Background="#FFD2B48C" CellSpacing="10" BorderThickness="5,5,5,5">
    <Table.Columns>
      <TableColumn Background="#FFADD8E6" />
      <TableColumn />
      <TableColumn />
    </Table.Columns>
    <Table.Resources>
      <Style TargetType="TableCell" x:Key="{x:Type TableCell}">
        <Style.Resources>
          <ResourceDictionary />
        </Style.Resources>
        <Setter Property="Block.BorderBrush">
          
            <SolidColorBrush>#FF000000</SolidColorBrush>
          
        </Setter>
        <Setter Property="Block.BorderThickness">
          
            <Thickness>2,2,2,2</Thickness>
          
        </Setter>
      </Style>
    </Table.Resources>
    <TableRowGroup Background="#FFFF0000">
      <TableRow Background="#FFFFFF00">
        <TableCell Background="#FF000000" RowSpan="2">
          <Paragraph Foreground="#FFFFFFFF">1</Paragraph>
        </TableCell>
        <TableCell>
          <Paragraph>2</Paragraph>
        </TableCell>
        <TableCell>
          <Paragraph>3</Paragraph>
        </TableCell>
      </TableRow>
      <TableRow>
        <TableCell>
          <Paragraph>4</Paragraph>
        </TableCell>
        <TableCell>
          <Paragraph>5</Paragraph>
        </TableCell>
      </TableRow>
    </TableRowGroup>
    <TableRowGroup>
      <TableRow>
        <TableCell>
          <Paragraph>6</Paragraph>
        </TableCell>
        <TableCell Background="#FF90EE90" ColumnSpan="2">
          <Paragraph>7</Paragraph>
        </TableCell>
      </TableRow>
      <TableRow>
        <TableCell ColumnSpan="2">
          <Paragraph>8</Paragraph>
        </TableCell>
        <TableCell>
          <Paragraph>9</Paragraph>
        </TableCell>
      </TableRow>
    </TableRowGroup>
  </Table>
</Section>