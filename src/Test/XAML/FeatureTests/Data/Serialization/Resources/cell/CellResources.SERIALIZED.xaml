<CustomDockPanel Background="#FFFFFFFF" LastChildFill="True" xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <av:FlowDocumentScrollViewer>
    <av:FlowDocument>
      <av:Table Margin="0,0,0,0">
        <av:Table.Columns>
          <av:TableColumn />
        </av:Table.Columns>
        <av:TableRowGroup>
          <av:TableRow>
            <av:TableRow.Resources>
              <av:SolidColorBrush x:Key="foreground">#FFFF0000</av:SolidColorBrush>
            </av:TableRow.Resources>
            <av:TableCell Foreground="{av:DynamicResource foreground}">
              <av:Paragraph Margin="0,0,0,0">The cell</av:Paragraph>
            </av:TableCell>
          </av:TableRow>
        </av:TableRowGroup>
      </av:Table>
    </av:FlowDocument>
  </av:FlowDocumentScrollViewer>
</CustomDockPanel>