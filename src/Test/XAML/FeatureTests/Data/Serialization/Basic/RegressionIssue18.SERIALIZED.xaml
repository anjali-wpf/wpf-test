<CustomDockPanel xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <av:Button>
    <av:Button.Resources>
      <av:Style TargetType="av:Button" x:Key="{x:Type av:Button}">
        <av:Style.Resources>
          <av:ResourceDictionary />
        </av:Style.Resources>
        <av:Setter Property="av:Panel.Background">
          <av:Setter.Value>
            <av:SolidColorBrush>#FF008000</av:SolidColorBrush>
          </av:Setter.Value>
        </av:Setter>
      </av:Style>
    </av:Button.Resources>
    <av:Button>i am inside</av:Button>
  </av:Button>
</CustomDockPanel>