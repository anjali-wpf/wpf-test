<CustomDockPanel xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:s="clr-namespace:System;assembly=mscorlib">
  <CustomDockPanel.Resources>
    <av:Style TargetType="av:Button" x:Key="{x:Type av:Button}">
      <av:Style.Resources>
        <av:ResourceDictionary />
      </av:Style.Resources>
      <av:Setter Property="av:Panel.Background">
        <av:Setter.Value>
          <av:SolidColorBrush>#FFFFFF00</av:SolidColorBrush>
        </av:Setter.Value>
      </av:Setter>
    </av:Style>
    <av:Style TargetType="av:ListBox" x:Key="{x:Type av:ListBox}">
      <av:Style.Resources>
        <av:ResourceDictionary />
      </av:Style.Resources>
      <av:Setter Property="av:Panel.Background">
        <av:Setter.Value>
          <av:SolidColorBrush>#FF0000FF</av:SolidColorBrush>
        </av:Setter.Value>
      </av:Setter>
    </av:Style>
    <av:SelectionMode x:Key="{x:Type av:SelectionMode}">Single</av:SelectionMode>
    <av:Color x:Key="{x:Type av:Color}">#FFFF0000</av:Color>
  </CustomDockPanel.Resources>
  <av:Button Foreground="#FFFF0000" Name="Button0">
    <x:Array Type="s:String">
      <s:String>Hello</s:String>
      <s:String>World</s:String>
    </x:Array>
  </av:Button>
  <av:ListBox SelectionMode="Single" Name="ListBox0" />
</CustomDockPanel>