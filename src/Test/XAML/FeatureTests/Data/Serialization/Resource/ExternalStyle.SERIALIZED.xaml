<DockPanel Name="body" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <Canvas>
    <Button Name="border1" Style="{DynamicResource CustomBackground}" />
    <DockPanel>
      <Button Name="dock1" Style="{DynamicResource BottomToolBar}">
        <Button Name="b1" Style="{DynamicResource ManageUsersButton}">Manage User Accounts</Button>
      </Button>
      <DockPanel LastChildFill="True">
        <Button Name="Logo" Style="{DynamicResource LOGO}" />
        <DockPanel LastChildFill="True">
          <Button Name="button1" Style="{DynamicResource VerticalDockStacker}">
            <TextBlock Text="Patrick" Name="t1" Style="{DynamicResource UserTileText}" />
          </Button>
        </DockPanel>
      </DockPanel>
    </DockPanel>
  </Canvas>
</DockPanel>