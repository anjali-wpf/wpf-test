<CustomCanvas xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <CustomCanvas.Background>
    <av:LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
      <av:LinearGradientBrush.GradientStops>
        <av:GradientStop Offset="0" Color="#FF008000" />
        <av:GradientStop Offset="1" Color="#FFFFFFFF" />
      </av:LinearGradientBrush.GradientStops>
    </av:LinearGradientBrush>
  </CustomCanvas.Background>
  <av:Decorator Name="TransformDecorator">
    <av:Decorator.LayoutTransform>
      <av:TransformGroup>
        <av:TransformGroup.Children>
          <av:SkewTransform AngleY="-20" />
          <av:SkewTransform AngleX="-20" />
        </av:TransformGroup.Children>
      </av:TransformGroup>
    </av:Decorator.LayoutTransform>
    <av:Rectangle Height="50" Width="150" Fill="#FFFFC0CB" Name="Rectangle" Stroke="#FFFF0000" StrokeThickness="3" av:Canvas.Left="5" av:Canvas.Top="10" />
  </av:Decorator>
</CustomCanvas>