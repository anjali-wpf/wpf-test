<CustomCanvas xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <av:Decorator Name="TransformDecorator">
    <av:Decorator.RenderTransform>
      <av:TransformGroup>
        <av:TransformGroup.Children>
          <av:TranslateTransform X="200" Y="0" />
          <av:ScaleTransform ScaleX="2" ScaleY="2" />
        </av:TransformGroup.Children>
      </av:TransformGroup>
    </av:Decorator.RenderTransform>
    <av:Polyline StrokeMiterLimit="1" Fill="#FFFFFF00" Name="Polyline" Stroke="#FF0000FF" Points="10,10 60,30 10,50 60,70 10,90 30,25" StrokeLineJoin="Miter" StrokeThickness="3" />
  </av:Decorator>
</CustomCanvas>