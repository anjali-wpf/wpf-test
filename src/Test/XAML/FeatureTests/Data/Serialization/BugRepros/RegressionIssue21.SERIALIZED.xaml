<CustomDockPanel xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:s="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <av:Border Background="#FFFFFFFF">
    <av:Border.Resources>
      <av:Style TargetType="av:StackPanel" x:Key="{x:Type av:StackPanel}">
        <av:Style.Resources>
          <av:ResourceDictionary />
        </av:Style.Resources>
        <av:Setter Property="av:FrameworkElement.Width">
          <av:Setter.Value>
            <s:Double>200</s:Double>
          </av:Setter.Value>
        </av:Setter>
        <av:Setter Property="av:Panel.Background">
          <av:Setter.Value>
            <av:SolidColorBrush>#FFFF0000</av:SolidColorBrush>
          </av:Setter.Value>
        </av:Setter>
      </av:Style>
    </av:Border.Resources>
    <av:StackPanel>
      <av:Border Background="#FFFFFF00">
        <av:TextBlock Text="1,1" />
      </av:Border>
      <av:Border Background="#FFDAA520">
        <av:TextBlock Text="1,2" />
      </av:Border>
      <av:Border Background="#FFDAA520">
        <av:TextBlock Text="2,1" />
      </av:Border>
      <av:Border Background="#FFFFFF00">
        <av:TextBlock Text="2,2" />
      </av:Border>
      <av:Border Background="#FFFFFF00">
        <av:TextBlock Text="3,1" />
      </av:Border>
      <av:Border Background="#FFDAA520">
        <av:TextBlock Text="3,2" />
      </av:Border>
      <av:Border Background="#FFDAA520">
        <av:TextBlock Text="4,1" />
      </av:Border>
      <av:Border Background="#FFFFFF00">
        <av:TextBlock Text="4,2" />
      </av:Border>
    </av:StackPanel>
  </av:Border>
</CustomDockPanel>