<!-- Verify Style.Setter parsing properly support Xml namespaces -->
<wpf:Page
    xmlns:wpf="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" >
    <wpf:Page.Resources>
        <wpf:Style TargetType='wpf:Button'>
            <wpf:Setter Property='Background' Value='Red' />
        </wpf:Style>
    </wpf:Page.Resources>
    <wpf:Button>Click</wpf:Button>
</wpf:Page>