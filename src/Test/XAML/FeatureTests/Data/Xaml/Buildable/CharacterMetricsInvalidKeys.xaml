<!-- CharacterMetrics objects with invalid keys -->
<Page   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"> 

    <Grid>
        <StackPanel Name="root"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xml:lang="en-US"
            Background="white">

            <TextBlock>
                <TextBlock.FontFamily>
                    <FontFamily
                        xmlns:System="clr-namespace:System;assembly=mscorlib"
                        Baseline="0.972" LineSpacing="1.2">

                        <FontFamily.FamilyNames>
                            <System:String x:Key="en-US">Antique Olive Compact (Xerox WorkCentre Pro 35 PS)</System:String>
                        </FontFamily.FamilyNames>

                        <FontFamily.FamilyTypefaces>
                            <FamilyTypeface
                                Weight="Normal"
                                Stretch="Normal"
                                DeviceFontName="Antique Olive Compact (Xerox WorkCentre Pro 35 PS)">
                            <FamilyTypeface.DeviceFontCharacterMetrics>
                                <CharacterMetrics x:Key="A31" Metrics="0.352,0.407,0.057" />
                                <CharacterMetrics x:Key="B32" Metrics="0.352,0.214,0.101" />
                                <CharacterMetrics x:Key="C33" Metrics="0.512,0.224,0.050" />
                                <CharacterMetrics x:Key="D34" Metrics="0.577,0.265,-0.486" />
                                <CharacterMetrics x:Key="E35" Metrics="0.704,0.712,0.050" />
                                <CharacterMetrics x:Key="F36" Metrics="0.704,0.598,0.050" />
                            </FamilyTypeface.DeviceFontCharacterMetrics>
                        </FamilyTypeface>
                        </FontFamily.FamilyTypefaces>

                        <FontFamily.FamilyMaps>
                            <FontFamilyMap Unicode="0000-052F" Target="Tahoma" Scale="1.0" />
                        </FontFamily.FamilyMaps>

                    </FontFamily>
              </TextBlock.FontFamily>
            Device Composite Font
            </TextBlock>

        </StackPanel>
    </Grid>

</Page>
