<Page
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml">

    <FlowDocumentScrollViewer
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        Name="ELEMENTFACTORY_PARENT_LINEBREAK">
        <FlowDocument>
            <Paragraph>
                LineBreak (START--)
                <LineBreak
                    Name="ELEMENTFACTORY_LINEBREAK"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" />
                (-- END) LineBreak
            </Paragraph>
        </FlowDocument>
    </FlowDocumentScrollViewer>

</Page>
