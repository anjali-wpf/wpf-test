<Window
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys   = "clr-namespace:System;assembly=mscorlib"
    xmlns:sn    = "clr-namespace:System.ComponentModel;assembly=System"
    xmlns:pc    = "clr-namespace:System.Windows.Media.Converters;assembly=PresentationCore"
    xmlns:md    = "clr-namespace:System.Windows.Media;assembly=PresentationCore"
    xmlns:im    = "clr-namespace:System.Windows.Media.Imaging;assembly=PresentationCore"
    xmlns:fr    = "clr-namespace:System.Windows.Markup;assembly=PresentationFramework"
    xmlns:ba    = "clr-namespace:System.Windows.Markup;assembly=WindowsBase"
    xmlns:ma    = "clr-namespace:System.Windows.Markup;assembly=PresentationCore"
    xmlns:re    = "clr-namespace:System.Windows.Resources;assembly=PresentationFramework">

    <Window.Resources>
        <BitmapImage UriSource="puppies.jpg" x:Key="t1" />
        <BooleanAnimationUsingKeyFrames x:Key="t2" />
        <BooleanKeyFrameCollection x:Key="t3" />
        <BmpBitmapEncoder x:Key="t4" />
        <BorderGapMaskConverter x:Key="t5" />
        <CharKeyFrameCollection x:Key="t6" />
        <CollectionContainer x:Key="t7" />
        <CollectionViewSource x:Key="t8" />
        <ColorAnimationUsingKeyFrames x:Key="t9" />
        <ColorKeyFrameCollection x:Key="t10" />
        <CroppedBitmap Source="pack://siteoforigin:,,,/puppies.jpg" x:Key="t11" />
        <CultureInfoIetfLanguageTagConverter x:Key="t12" />
        <DashStyle x:Key="t13" />

        <sys:DateTime x:Key="t14" />
        <DiscreteBooleanKeyFrame x:Key="t15" />
        <DiscreteColorKeyFrame x:Key="t16" />
        <DiscreteStringKeyFrame x:Key="t17" />
        <DoubleAnimationUsingPath x:Key="t18" />
        <Duration x:Key="t19" />
        <EmissiveMaterial x:Key="t20" />
        <EventSetter x:Key="t21" />
        <FontSizeConverter x:Key="t22" />
        <GifBitmapEncoder x:Key="t23" />
        <HierarchicalDataTemplate x:Key="t24" />
        <Int32CollectionConverter x:Key="t25" />
        <ItemsPanelTemplate x:Key="t26" />
        <JournalEntryUnifiedViewConverter x:Key="t27" />
        <JpegBitmapEncoder x:Key="t28" />
        <KeyBinding x:Key="t29" />
        <LengthConverter x:Key="t30" />
        <LinearColorKeyFrame x:Key="t31" />
        <LinearDoubleKeyFrame x:Key="t32" />
        <MaterialCollection x:Key="t33" />
        <MatrixAnimationUsingPath x:Key="t34" />
        <MediaTimeline x:Key="t35" />
        <MenuScrollingVisibilityConverter x:Key="t36" />
        <MultiDataTrigger x:Key="t37" />
        <ObjectDataProvider x:Key="t38" />
        <PngBitmapEncoder x:Key="t39" />
        <PointAnimationUsingPath x:Key="t40" />
        <PointCollectionConverter x:Key="t41" />
        <Rect x:Key="t42" />
        <SoundPlayerAction x:Key="t43" />
        <SpecularMaterial x:Key="t44" />
        <SplineColorKeyFrame x:Key="t45" />
        <StringAnimationUsingKeyFrames x:Key="t46" />
        <StringKeyFrameCollection x:Key="t47" />
        <TextEffect x:Key="t48" />
        <TiffBitmapEncoder x:Key="t49" />
        <sys:TimeSpan x:Key="t50" />
        <Transform3DCollection x:Key="t51" />
        <TransformedBitmap Source="pack://siteoforigin:,,,/puppies.jpg" x:Key="t52" />
        <WmpBitmapEncoder x:Key="t53" />
        <XmlNamespaceMapping Prefix="y" Uri="" x:Key="t54" />
        <BeginStoryboard x:Key="t55" />
        <PauseStoryboard x:Key="t56" />
        <RemoveStoryboard x:Key="t57" />
        <ResumeStoryboard x:Key="t58" />
        <SeekStoryboard x:Key="t59" />
        <SetStoryboardSpeedRatio x:Key="t60" />
        <SkipStoryboardToFill x:Key="t61" />
        <StopStoryboard x:Key="t62" />

        <sn:CharConverter x:Key="con1" />
        <sn:ByteConverter x:Key="con2" />
        <sn:BooleanConverter x:Key="con3" />
        <sn:CultureInfoConverter x:Key="con4" />
        <sn:DateTimeConverter x:Key="con5" />
        <sn:DecimalConverter x:Key="con6" />
        <sn:DoubleConverter x:Key="con7" />
        <sn:GuidConverter x:Key="con8" />
        <sn:Int16Converter x:Key="con9" />
        <sn:Int64Converter x:Key="con10" />
        <sn:SByteConverter x:Key="con11" />
        <sn:SingleConverter x:Key="con12" />
        <sn:StringConverter x:Key="con13" />
        <sn:TimeSpanConverter x:Key="con14" />
        <sn:UInt16Converter x:Key="con15" />
        <sn:UInt32Converter x:Key="con16" />
        <sn:UInt64Converter x:Key="con17" />
        <pc:BoolIListConverter x:Key="con18" />
        <pc:CharIListConverter x:Key="con19" />
        <pc:DoubleIListConverter x:Key="con20" />
        <pc:UShortIListConverter x:Key="con21" />
        <fr:ComponentResourceKeyConverter x:Key="con22" />
        <fr:ParserContext x:Key="tcon23" />
        <fr:RoutedEventConverter x:Key="con24" />
        <fr:TemplateKeyConverter x:Key="con25" />
        <fr:XamlReader x:Key="con26" />
        <fr:DependencyPropertyConverter x:Key="con27" />
        <md:FontFamilyConverter x:Key="con28" />
        <im:FormatConvertedBitmap Source="pack://siteoforigin:,,,/puppies.jpg" x:Key="con29" />
        <re:StreamResourceInfo x:Key="con30" />
        <ba:WhitespaceSignificantCollectionAttribute x:Key="con31" />
        <ba:ContentPropertyAttribute x:Key="con32" />
        <ma:XmlLanguageConverter x:Key="con33" />

        <ControlTemplate x:Key="temp1">
            <DockPanel>
                <TextBlock Foreground="Red" FontSize="20">!</TextBlock>
                <AdornedElementPlaceholder/>
            </DockPanel>
        </ControlTemplate>

        <Storyboard x:Key="story1">
            <ThicknessAnimation
                Storyboard.TargetProperty="(ListBox.Padding)"
                Duration="00:00:1.5"
                To="101" />
        </Storyboard>  

        <Storyboard x:Key="story2">
            <Point3DAnimation
                    To          = "{Binding ElementName=textblock, Path='Text'}"
                    BeginTime   = "0:0:1"
                    Duration    = "0:0:2"
                    />
        </Storyboard>

        <Storyboard x:Key="story3">
            <PointAnimation
                Storyboard.TargetProperty="(Path.Data).(LineGeometry.StartPoint)"
                BeginTime="0:0:3"
                Duration="0:0:2"
                From="200,50"
                To="{Binding Path='Content'}"
                  />
        </Storyboard>

        <Storyboard x:Key="story4">
            <RectAnimation
                Storyboard.TargetProperty   = "(Path.Data).(RectangleGeometry.Rect)"
                BeginTime                   = "0:0:2"
                Duration                    = "0:0:4"
                To                          = "0,0,100,100"
                  />
        </Storyboard>  

        <Storyboard x:Key="story5">
            <Rotation3DAnimationUsingKeyFrames BeginTime="00:00:01" Duration="00:00:02" Storyboard.TargetName="Viewport3D" Storyboard.TargetProperty="(Viewport3D.Children)[0].(ModelVisual3D.Content).(Model3DGroup.Children)[2].(Model3D.Transform).(Transform3DGroup.Children)[2].(RotateTransform3D.Rotation)">
                <SplineRotation3DKeyFrame KeySpline="0.5,0.5,0.5,0.5" KeyTime="00:00:01">
                    <SplineRotation3DKeyFrame.Value>
                        <AxisAngleRotation3D Axis="-1,0,0" Angle="30"/>
                    </SplineRotation3DKeyFrame.Value>
                </SplineRotation3DKeyFrame>
            </Rotation3DAnimationUsingKeyFrames>
        </Storyboard>

        <TextDecorationCollection x:Key="dec1">
            <TextDecoration Location="Underline"/>
        </TextDecorationCollection>

        <TextBlock x:Key="tb1" Grid.Column="1" Margin="3">
            <TextBlock.Text>
                <MultiBinding StringFormat="{}{0} {1}">
                    <Binding Path="FirstName"/>
                    <Binding Path="LastName"/>
                </MultiBinding>
            </TextBlock.Text>
        </TextBlock>

        <TextBlock x:Key="tb2" Background="Honeydew" Width="100">
            <TextBlock.Text>
                <PriorityBinding FallbackValue="defaultvalue">
                    <Binding Path="SlowestDP" IsAsync="True"/>
                    <Binding Path="SlowerDP" IsAsync="True"/>
                    <Binding Path="FastDP" />
                </PriorityBinding>
            </TextBlock.Text>
        </TextBlock>

    </Window.Resources>

</Window>