                BeginTime                   = "00:0:2"
                Duration                    = "0:0:4"
                FillBehavior                = "HoldEnd"
                >
                <PointAnimationUsingKeyFrames.KeyFrames>
                      <PointKeyFrameCollection>
                           <DiscretePointKeyFrame Value="300,100" KeyTime="0" />
                           <LinearPointKeyFrame Value="100,100"   KeyTime="50%" />
                           <SplinePointKeyFrame   />
