                           <LinearInt32KeyFrame   />
                           <SplineInt32KeyFrame   />
                      </Int32KeyFrameCollection>
                 </Int32AnimationUsingKeyFrames.KeyFrames>
            </Int32AnimationUsingKeyFrames>
        </Storyboard>

        <Storyboard x:Key="Story3">
            <Point3DAnimationUsingKeyFrames Storyboard.TargetProperty="(PerspectiveCamera.Position)">
                 <Point3DAnimationUsingKeyFrames.KeyFrames>
