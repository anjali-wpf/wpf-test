<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    
    <sys:String x:Key="SomeText">Some Text</sys:String>
    <sys:Double x:Key="Pi">3.14</sys:Double>
    <sys:Int32 x:Key="Ten">10</sys:Int32>
    <sys:Double x:Key="DotFive">0.5</sys:Double>
    
    <sys:Byte x:Key="Byte0">0</sys:Byte>
    <sys:Byte x:Key="Byte128">128</sys:Byte>
    <sys:Byte x:Key="Byte255">255</sys:Byte>
</ResourceDictionary>