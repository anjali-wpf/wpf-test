<Page
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:i1="unknown" 
    xmlns:Understandable="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:Understandable1="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    >

  <StackPanel>

    <mc:AlternateContent>
      <mc:Choice Requires="i1" >
        <i1:TextBlock mc:MustUnderstand="i1">I1 textblock</i1:TextBlock>
      </mc:Choice>
    </mc:AlternateContent>

    <mc:AlternateContent>
      <mc:Choice Requires="Understandable Understandable1" >
        <TextBlock>Understandable textblock</TextBlock>
      </mc:Choice>
    </mc:AlternateContent>

  </StackPanel>

</Page>
