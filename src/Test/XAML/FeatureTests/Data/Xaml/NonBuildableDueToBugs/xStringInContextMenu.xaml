<!-- x:String inside a ContextMenu -->
<Page
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml">

    <TextBox Name="TextBox2" Width="250" Height="100">
        <TextBox.ContextMenu>
            <ContextMenu>
                <x:String>Wrapper Item 1</x:String>
                <x:String>Wrapper Item 2</x:String>
                <MenuItem Header="Disabled menu item" IsEnabled="false" />
                <x:String>Wrapper Item 3</x:String>
            </ContextMenu>
        </TextBox.ContextMenu>
    </TextBox>

</Page>