<!-- x:Key Directive Property Element for a RadialGradientBrush in different combination with other properties -->

<StackPanel 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <StackPanel.Resources>

        <RadialGradientBrush><x:Key>Value1</x:Key></RadialGradientBrush>

        <RadialGradientBrush>
            <x:Key>Value2</x:Key>
        </RadialGradientBrush>

        <RadialGradientBrush GradientOrigin="0,0"><x:Key>Value3</x:Key></RadialGradientBrush>

        <RadialGradientBrush GradientOrigin="0,0">
            <x:Key>Value4</x:Key>
            <RadialGradientBrush.GradientStops><GradientStop Color="DarkSeaGreen" Offset="0.5" /></RadialGradientBrush.GradientStops>
        </RadialGradientBrush>

        <RadialGradientBrush>
            <RadialGradientBrush.GradientOrigin>0,0</RadialGradientBrush.GradientOrigin>
            <RadialGradientBrush.GradientStops><GradientStop Color="Turquoise" Offset="0.5" /></RadialGradientBrush.GradientStops>
            <x:Key>Value5</x:Key>
        </RadialGradientBrush>

        <RadialGradientBrush>
            <RadialGradientBrush.GradientOrigin>0,0</RadialGradientBrush.GradientOrigin>
            <x:Key>Value6</x:Key>
            <RadialGradientBrush.GradientStops><GradientStop Color="LightGreen" Offset="0.5" /></RadialGradientBrush.GradientStops>
        </RadialGradientBrush>

        <RadialGradientBrush>
            <x:Key>Value7</x:Key>
            <RadialGradientBrush.GradientOrigin>0,0</RadialGradientBrush.GradientOrigin>
            <RadialGradientBrush.GradientStops><GradientStop Color="ForestGreen" Offset="0.5" /></RadialGradientBrush.GradientStops>
        </RadialGradientBrush>


    </StackPanel.Resources>

</StackPanel>