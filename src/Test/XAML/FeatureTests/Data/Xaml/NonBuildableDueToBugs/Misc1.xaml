<!-- x:Key Directive Property Element with XmlDataProvider / xmlns="" -->
<Page
    xmlns   = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x = "http://schemas.microsoft.com/winfx/2006/xaml">
    <DockPanel>
        <DockPanel.Resources>
            <XmlDataProvider XPath="/root">
                <x:Key>Key1</x:Key>
                <x:XData>
                    <root xmlns="">
                        <brush>Red</brush>
                    </root>
                </x:XData>
            </XmlDataProvider>
        </DockPanel.Resources>
    </DockPanel>
</Page>
