<!-- x:Key Directive Property Element on a  ControlTemplate / DataTemplate / Storyboard / XmlDataProvider / etc. -->
<Expander
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml">

    <Expander.Resources>
        <ParallelTimeline><x:Key>Value1</x:Key></ParallelTimeline>
        <ControlTemplate TargetType="{x:Type Button}"><x:Key>Value2</x:Key></ControlTemplate>
        <DataTemplate><x:Key>Value3</x:Key></DataTemplate>
        <Storyboard><x:Key>Value4</x:Key></Storyboard>
        <XmlDataProvider><x:Key>Value5</x:Key></XmlDataProvider>
        <Vector3DCollection><x:Key>Value6</x:Key>
            <StaticResource ResourceKey="VectorA"/>
            <StaticResource ResourceKey="VectorB"/>
        </Vector3DCollection>
        <Int32Collection><x:Key>Value7</x:Key><StaticResource ResourceKey="IntA"/></Int32Collection>
        <CollectionViewSource><x:Key>Value8</x:Key></CollectionViewSource>
        <BooleanToVisibilityConverter><x:Key>Value9</x:Key></BooleanToVisibilityConverter>
        <Dock x:Key="Value10">Top</Dock>
        <Dock><x:Key>Value11</x:Key>Bottom</Dock>
        <Orientation><x:Key>Value12</x:Key>Horizontal</Orientation>
        <Color x:Key="{}{Blue}{Blue}" B="222" A="255" />
    </Expander.Resources>

</Expander>