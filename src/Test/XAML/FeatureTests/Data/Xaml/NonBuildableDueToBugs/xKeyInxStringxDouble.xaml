<!-- x:Key Directive Property Element - inside x:Double / x:String -->
<Canvas
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:sys   = "clr-namespace:System;assembly=Mscorlib"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml"> 

    <Canvas.Resources>

        <ResourceDictionary>
            <x:Double><x:Key>Value1</x:Key></x:Double>

            <x:Double>
                <x:Key>
                Value2
                </x:Key>
            </x:Double>

            <x:String><x:Key>Value3</x:Key></x:String>

            <x:String>
                <x:Key>
                Value4
                </x:Key>
            </x:String>

        </ResourceDictionary>

    </Canvas.Resources>

    <ComboBox>
        <x:String>
            <x:Key>Key2</x:Key>
            RedBrush
        </x:String> 
    </ComboBox>

</Canvas>