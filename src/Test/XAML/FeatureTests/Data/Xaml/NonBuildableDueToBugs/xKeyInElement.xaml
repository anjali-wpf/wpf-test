<!-- x:Key Directive Property Element inside a Button and with x:Null -->
<DockPanel
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml">

    <Button>
        <x:Key>KeyValue</x:Key>   
    </Button>

    <Button>
        <x:Key><x:Null /></x:Key>   
    </Button>

</DockPanel>
