    xmlns:sys   = "clr-namespace:System;assembly=Mscorlib"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml">

    <x:Arguments></x:Arguments>

    <x:Arguments>
       <Button />
    </x:Arguments>

    <InkCanvas>
        <x:Arguments></x:Arguments>
    </InkCanvas>

    <Button>
        <x:Arguments> </x:Arguments>
    </Button>

    <ListBox>
        <x:Arguments>abc</x:Arguments>
    </ListBox>

    <DockPanel>
        <x:Arguments>
            {ct:CustomME_AmbientValue Text}
        </x:Arguments>
    </DockPanel>

    <CustomTag x:Arguments="abc" />

    <x:Double x:FactoryMethod="abc">
        <x:Arguments></x:Arguments>
    </x:Double>

    <x:Double x:FactoryMethod="abc">
        <x:Arguments><x:Boolean>True</x:Boolean></x:Arguments>
    </x:Double>

    <Expander>
        <x:Arguments>
            <sys:Double><x:Key>Value1</x:Key></sys:Double> 
        </x:Arguments>
    </Expander>

</Page>