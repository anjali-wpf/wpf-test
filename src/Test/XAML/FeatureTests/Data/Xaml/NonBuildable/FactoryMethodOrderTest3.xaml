<!-- Scenario: with x:Arguments and PropertyElements both before and after -->
<Page
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:co    = "http://XamlTestTypes">

    <co:CustomFactoryWithArgAndProps BooleanProp="true" x:FactoryMethod="Factory" StringProp="wpf">
        <co:CustomFactoryWithArgAndProps.IntProp>100</co:CustomFactoryWithArgAndProps.IntProp>
            <x:Arguments>
                <x:String>MyString</x:String>
            </x:Arguments>
        <co:CustomFactoryWithArgAndProps.DoubleProp>100</co:CustomFactoryWithArgAndProps.DoubleProp>
    </co:CustomFactoryWithArgAndProps>

</Page>