<!-- Scenario: with x:Arguments following a PropertyElement -->
<Page
    xmlns       = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x     = "http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:co    = "http://XamlTestTypes">

    <co:CustomFactoryWithArgAndProps IntProp="300" x:FactoryMethod="Factory" BooleanProp="false">
        <co:CustomFactoryWithArgAndProps.DoubleProp>100</co:CustomFactoryWithArgAndProps.DoubleProp>
        <x:Arguments>
            <x:String>AString</x:String>
        </x:Arguments>
    </co:CustomFactoryWithArgAndProps>

</Page>