<!-- Harmful in partial trust - this xaml deletes a file (itself) -->    
<x:String xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:sio="clr-namespace:System.IO;assembly=mscorlib"
          x:FactoryMethod="sio:File.Delete">
   <x:Arguments>
      <x:String>HarmfulInPartialTrust.xaml</x:String>
   </x:Arguments>
</x:String>