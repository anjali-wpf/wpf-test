    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"  >
    <FlowDocument>
        <Paragraph>
            Hello
            <Bold>World</Bold> , how are you?
            <Paragraph.Tag>23</Paragraph.Tag>
        </Paragraph>
    </FlowDocument>
</Page>