<Project DefaultTargets="Clean;Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Platform>AnyCPU</Platform>
    <AssemblyName>Modifier</AssemblyName>
    <Configuration>Release</Configuration>
    <OutputType>winexe</OutputType>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <RootNamespace>
    </RootNamespace>
    <OptionStrict>On</OptionStrict>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.targets" />
  <Import Project="$(MSBuildBinPath)\Microsoft.WinFX.targets" />
  <ItemGroup>
    <ApplicationDefinition Include="ModifierApp.xaml" />
    <Page Include="ModifierMarkup.xaml" />
    <Compile Include="ModifierCode.cs" />
  </ItemGroup>
  <!-- Included only for test run -->
  <Import Project="TestCommon.target" />
  <ItemGroup>
    <Reference Include="System" />
    
    <Reference Include="System.Data" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="UIAutomationClient" />
    <Reference Include="UIAutomationTypes" />
    <Reference Include="UIAutomationProvider" />
  </ItemGroup>
</Project>
