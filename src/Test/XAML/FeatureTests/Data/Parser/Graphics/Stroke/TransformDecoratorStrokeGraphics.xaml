<cmn:CustomCanvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cc="http://XamlTestTypes" xmlns:cmn="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime"  xmlns:MappingPIGen2="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime">
    <Decorator Name="TransformDecorator">
        <Decorator.RenderTransform>
            <TransformGroup>
                <TransformGroup.Children>
                    <TransformCollection>
                        <TranslateTransform X="200" Y="0" />
                        <ScaleTransform ScaleX="2" ScaleY="2" />
                    </TransformCollection>
                </TransformGroup.Children>
            </TransformGroup>
        </Decorator.RenderTransform>
        <Polyline Name="Polyline">
            <Polyline.Points>
          10,10 60,30 10,50 60,70
          10,90 30,25
        </Polyline.Points>
            <Polyline.StrokeThickness>3</Polyline.StrokeThickness>
            <Polyline.StrokeLineJoin>miter</Polyline.StrokeLineJoin>
            <Polyline.StrokeMiterLimit>1</Polyline.StrokeMiterLimit>
            <Polyline.Stroke>blue</Polyline.Stroke>
            <Polyline.Fill>yellow</Polyline.Fill>
        </Polyline>
    </Decorator>
</cmn:CustomCanvas>
