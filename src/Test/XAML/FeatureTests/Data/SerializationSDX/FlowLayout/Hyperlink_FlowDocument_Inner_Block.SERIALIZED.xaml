<FlowDocumentPageViewer xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <FlowDocument>
    <Section>
      <Paragraph>Page1</Paragraph>
      <Paragraph>
        <Hyperlink NavigateUri="pack://siteoforigin:,,,/Hyperlink_FlowDocument_Inner_Block.xaml#here" Name="HyperlinkObject">Navigate to 5th page of FlowDocument!</Hyperlink>
      </Paragraph>
      <Paragraph BreakPageBefore="True">Page2</Paragraph>
      <Paragraph BreakPageBefore="True">Page3</Paragraph>
      <Paragraph BreakPageBefore="True">Page4</Paragraph>
      <Paragraph Name="here" BreakPageBefore="True">Page5 Navigate here!</Paragraph>
      <Paragraph BreakPageBefore="True">Page6</Paragraph>
    </Section>
  </FlowDocument>
</FlowDocumentPageViewer>