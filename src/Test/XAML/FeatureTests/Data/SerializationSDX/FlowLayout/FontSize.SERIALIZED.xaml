<UniformGrid Background="#FFFFFFE0" FirstColumn="0" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:s="clr-namespace:System;assembly=mscorlib">
  <UniformGrid.Resources>
    <Style TargetType="FlowDocumentScrollViewer" x:Key="{x:Type FlowDocumentScrollViewer}">
      <Style.Resources>
        <ResourceDictionary />
      </Style.Resources>
      <Setter Property="ScrollViewer.VerticalScrollBarVisibility">
        <Setter.Value>
          <x:Static Member="ScrollBarVisibility.Hidden" />
        </Setter.Value>
      </Setter>
    </Style>
    <Style TargetType="FlowDocument" x:Key="{x:Type FlowDocument}">
      <Style.Resources>
        <ResourceDictionary />
      </Style.Resources>
      <Setter Property="TextElement.FontSize">
        <Setter.Value>
          <s:Double>25</s:Double>
        </Setter.Value>
      </Setter>
    </Style>
  </UniformGrid.Resources>
  <FlowDocumentScrollViewer>
    <FlowDocument Name="auto" Background="#FFADD8E6">
      <Paragraph Margin="0,0,0,0">auto</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument Name="pixel" Background="#FFFFFFE0">
      <Paragraph Margin="0,0,0,0">pixel</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument Name="percent" Background="#FFADD8E6">
      <Paragraph Margin="0,0,0,0">percent</Paragraph>
    </FlowDocument>
  </FlowDocumentScrollViewer>
  <FlowDocumentScrollViewer>
    <FlowDocument Name="large" Background="#FFFFFFE0">
