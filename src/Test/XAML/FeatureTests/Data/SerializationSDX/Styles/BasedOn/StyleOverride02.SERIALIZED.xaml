<CustomStackPanel xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <CustomStackPanel.Resources>
    <av:Style TargetType="av:Button" x:Key="Btn">
      <av:Style.Resources>
        <av:ResourceDictionary />
      </av:Style.Resources>
      <av:Setter Property="av:Panel.Background">
        <av:Setter.Value>
          <av:SolidColorBrush>#FFFF0000</av:SolidColorBrush>
        </av:Setter.Value>
      </av:Setter>
    </av:Style>
  </CustomStackPanel.Resources>
  <av:Button Focusable="False" Style="{av:DynamicResource Btn}" />
  <av:Button Background="#FFFFFF00" Focusable="False" Style="{av:DynamicResource Btn}" />
</CustomStackPanel>