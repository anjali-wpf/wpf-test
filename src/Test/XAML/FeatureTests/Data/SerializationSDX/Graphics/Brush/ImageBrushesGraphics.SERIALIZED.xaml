<CustomCanvas xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <CustomCanvas.Background>
    <av:LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
      <av:GradientStopCollection>
        <av:GradientStop Color="#FF0000FF" Offset="0" />
        <av:GradientStop Color="#FFFFFFFF" Offset="1" />
      </av:GradientStopCollection>
    </av:LinearGradientBrush>
  </CustomCanvas.Background>
  <av:Rectangle Height="200" Margin="2,2,2,2" Name="Rectangle1" Stroke="#FFFFFFFF" StrokeThickness="2" Width="150">
    <av:Rectangle.Fill>
      <av:ImageBrush ImageSource="pack://siteoforigin:,,,/puppies.jpg" Stretch="UniformToFill" Viewport="0,0,1,1" ViewportUnits="RelativeToBoundingBox" />
    </av:Rectangle.Fill>
  </av:Rectangle>
  <av:Rectangle Height="100" Name="Rectangle2" Stroke="#FF000000" StrokeThickness="2" Width="200">
    <av:Rectangle.Fill>
      <av:ImageBrush ImageSource="pack://siteoforigin:,,,/png.png" TileMode="Tile" Viewport="0,0,0.3,0.3" />
    </av:Rectangle.Fill>
  </av:Rectangle>
</CustomCanvas>