              </av:Storyboard>
            </av:BeginStoryboard>
          </av:EventTrigger>
        </av:Style.Triggers>
        <av:Setter Property="av:Panel.Background">
          <av:Setter.Value>
            <av:SolidColorBrush>#FFFF0000</av:SolidColorBrush>
          </av:Setter.Value>
        </av:Setter>
      </av:Style>
