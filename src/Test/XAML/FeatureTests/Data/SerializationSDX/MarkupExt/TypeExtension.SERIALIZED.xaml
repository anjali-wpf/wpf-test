<CustomDockPanel xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <CustomDockPanel.Resources>
    <av:ResourceDictionary>
      <av:Style TargetType="av:Button">
        <av:Style.Resources>
          <av:ResourceDictionary />
        </av:Style.Resources>
        <av:Setter Property="av:Panel.Background">
          <av:Setter.Value>
            <av:SolidColorBrush>#FFFFFF00</av:SolidColorBrush>
          </av:Setter.Value>
        </av:Setter>
      </av:Style>
      <av:Color x:Key="{x:Type av:Color}">#FFFF0000</av:Color>
      <av:Style TargetType="av:ListBox">
        <av:Style.Resources>
          <av:ResourceDictionary />
        </av:Style.Resources>
        <av:Setter Property="av:Panel.Background">
          <av:Setter.Value>
            <av:SolidColorBrush>#FF0000FF</av:SolidColorBrush>
          </av:Setter.Value>
        </av:Setter>
      </av:Style>
      <av:SelectionMode x:Key="{x:Type av:SelectionMode}">Single</av:SelectionMode>
    </av:ResourceDictionary>
  </CustomDockPanel.Resources>
  <av:Button Foreground="#FFFF0000" Name="Button0">
    <x:Array Type="x:String">
      <x:String>Hello</x:String>
      <x:String>World</x:String>
    </x:Array>
  </av:Button>
  <av:ListBox Name="ListBox0" SelectionMode="Single" />
</CustomDockPanel>