<DockPanel xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <DockPanel.Resources>
    <ResourceDictionary>
      <Style TargetType="TextBlock">
        <x:Key>
          <x:String>HYPERLINK_GRAY_INDENT</x:String>
        </x:Key>
        <Style.Resources>
          <ResourceDictionary />
        </Style.Resources>
        <Style.Triggers>
          <Trigger Property="UIElement.IsMouseOver">
            <Setter Property="Inline.TextDecorations">
              <Setter.Value>
                <TextDecorationCollection>
                  <TextDecoration Location="Underline" />
                </TextDecorationCollection>
              </Setter.Value>
            </Setter>
            <Trigger.Value>
              <x:Boolean>True</x:Boolean>
            </Trigger.Value>
          </Trigger>
        </Style.Triggers>
      </Style>
      <Style TargetType="TextBlock">
        <x:Key>
          <x:String>AS_HYPERLINK</x:String>
        </x:Key>
        <Style.Resources>
          <ResourceDictionary />
        </Style.Resources>
        <Style.Triggers>
          <Trigger Property="UIElement.IsMouseOver">
            <Setter Property="Inline.TextDecorations">
              <Setter.Value>
                <TextDecorationCollection>
                  <TextDecoration Location="Underline" />
                </TextDecorationCollection>
              </Setter.Value>
            </Setter>
            <Trigger.Value>
              <x:Boolean>True</x:Boolean>
            </Trigger.Value>
          </Trigger>
        </Style.Triggers>
      </Style>
    </ResourceDictionary>
  </DockPanel.Resources>
</DockPanel>