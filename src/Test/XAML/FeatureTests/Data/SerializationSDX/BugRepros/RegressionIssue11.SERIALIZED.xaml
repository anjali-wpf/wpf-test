<CustomDockPanel Background="#FFCCCCCC" Name="DOCKPANEL1" xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <CustomDockPanel.Triggers>
    <av:EventTrigger RoutedEvent="av:FrameworkElement.Loaded">
      <av:BeginStoryboard>
        <av:Storyboard>
          <av:PointAnimation BeginTime="00:00:05" Duration="00:00:02" FillBehavior="HoldEnd" From="100,150" To="200,150" />
          <av:PointAnimation BeginTime="00:00:05" Duration="00:00:02" FillBehavior="HoldEnd" From="100,150" To="200,150" />
        </av:Storyboard>
      </av:BeginStoryboard>
    </av:EventTrigger>
  </CustomDockPanel.Triggers>
  <av:Path Name="Animate" Stroke="#FFFF0000" StrokeThickness="50">
    <av:Path.Data>
      <av:LineGeometry EndPoint="300,150" StartPoint="100,150" />
    </av:Path.Data>
  </av:Path>
</CustomDockPanel>