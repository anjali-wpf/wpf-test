<CustomDockPanel xmlns="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <CustomDockPanel.Triggers>
    <av:EventTrigger RoutedEvent="av:FrameworkElement.Loaded">
      <av:BeginStoryboard>
        <av:Storyboard BeginTime="01:00:00" Duration="Forever">
          <av:ColorAnimation AutoReverse="True" BeginTime="00:00:00" Duration="00:00:05" From="#FFFF0000" RepeatBehavior="100x" To="#FF0000FF" />
        </av:Storyboard>
      </av:BeginStoryboard>
    </av:EventTrigger>
  </CustomDockPanel.Triggers>
  <av:Ellipse Fill="#FF008000" />
  <av:Ellipse Fill="#FFFF0000" Name="Ellipse1" />
</CustomDockPanel>