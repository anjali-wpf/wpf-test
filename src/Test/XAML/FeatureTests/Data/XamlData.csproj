<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <NoTargets>true</NoTargets>
  </PropertyGroup>
  <ItemGroup>
      <None Include="Baml\**\*.*">
          <Link>\Data\Baml\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="Common\*.*">
          <Link>\Data\Common\%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="Localization\LocBamlData\**\*.*">
          <Link>\Data\Localization\LocBamlData\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="MarkupCompiler\Data\**\*.*">
          <Link>\MarkupCompiler\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="MarkupCompiler\**\*.*" Exclude="MarkupCompiler\Data\**\*.*">
          <Link>\MarkupCompiler\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="Parser\**\*.*">
          <Link>\Data\Parser\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="Serialization\**\*.*">
          <Link>\Data\Serialization\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="SerializationSDX\**\*.*">
          <Link>\Data\SerializationSDX\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="Xaml\**\*.*">
          <Link>\Data\Xaml\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="XamlXmlReader\**\*.*">
          <Link>\Data\XamlXmlReader\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Include="XTC\**\*.xtc">
          <Link>\Data\XTC\%(RecursiveDir)%(FileName)%(Extension)</Link>
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
  </ItemGroup>
</Project>
